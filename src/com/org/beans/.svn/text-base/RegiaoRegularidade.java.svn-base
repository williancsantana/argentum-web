package com.org.beans;

import com.org.model.classes.Municipio;
import com.org.model.classes.UF;
import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author Taidson
 */
public class RegiaoRegularidade {
    private String nome;
    private List<Municipio> listaMunicipio = new ArrayList<Municipio>();
    private List<UF> listaUF = new ArrayList<UF>();
    private int qtdeMunicipio;
    private int qtdeMunicIrregular;
    private Double percetualIrregular;
    
    public RegiaoRegularidade() {
    }

    public RegiaoRegularidade(String nome, List<Municipio> listaMunicipio) {
        this.nome = nome;
        this.listaMunicipio = listaMunicipio;
    }
    
    public List<Municipio> getListaMunicipio() {
        return listaMunicipio;
    }

    public String getNome() {
        return nome;
    }

    public void setListaMunicipio(List<Municipio> listaMunicipio) {
        this.listaMunicipio = listaMunicipio;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public List<UF> getListaUF() {
        return listaUF;
    }

    public void setListaUF(List<UF> listaUF) {
        this.listaUF = listaUF;
    }

    public int getQtdeMunicIrregular() {
        return qtdeMunicIrregular;
    }

    public void setQtdeMunicIrregular(int qtdeMunicIrregular) {
        this.qtdeMunicIrregular = qtdeMunicIrregular;
    }

    public int getQtdeMunicipio() {
        return qtdeMunicipio;
    }

    public void setQtdeMunicipio(int qtdeMunicipio) {
        this.qtdeMunicipio = qtdeMunicipio;
    }

    public Double getPercetualIrregular() {
        return percetualIrregular;
    }

    public void setPercetualIrregular(Double percetualIrregular) {
        this.percetualIrregular = percetualIrregular;
    }
    
    
}

package com.org.model.classes;

/**
 *
 * @author geraldo
 */
public class ColunasDbf {
private String nomeColuna;
private int tamanho;
private String tipo;
private int precisao;

public ColunasDbf( int tamanho){    
    this.tipo="C";
    this.tamanho=tamanho;
}
public ColunasDbf(int tamanho,int precisao){
    this.precisao=precisao;
    this.tipo="N";
    this.tamanho=tamanho;
}
    /**
     * @return the nomeColuna
     */
    public String getNomeColuna() {
        return nomeColuna;
    }

    /**
     * @param nomeColuna the nomeColuna to set
     */
    public void setNomeColuna(String nomeColuna) {
        this.nomeColuna = nomeColuna;
    }

    /**
     * @return the tamanho
     */
    public int getTamanho() {
        return tamanho;
    }

    /**
     * @param tamanho the tamanho to set
     */
    public void setTamanho(int tamanho) {
        this.tamanho = tamanho;
    }

    /**
     * @return the tipo
     */
    public String getTipo() {
        return tipo;
    }

    /**
     * @param tipo the tipo to set
     */
    public void setTipo(String tipo) {
        this.tipo = tipo;
    }

    /**
     * @return the precisao
     */
    public int getPrecisao() {
        return precisao;
    }

    /**
     * @param precisao the precisao to set
     */
    public void setPrecisao(int precisao) {
        this.precisao = precisao;
    }
}
